{
  "name": "lifion-insights",
  "version": "0.0.0",
  "description": "Tools for scanning repositories and static site generator",
  "keywords": [
    "advisories",
    "code",
    "insights",
    "quality",
    "repositories"
  ],
  "author": "Edgardo Avilés <Edgardo.Aviles@ADP.com>",
  "maintainers": [
    "Chen Doron <Chen.Doron@ADP.com>",
    "Edgardo Avilés <Edgardo.Aviles@ADP.com>",
    "Jenny Eckstein <Jenny.Eckstein@ADP.com>",
    "Mackenzie Turner <turner.mackenzie.m@gmail.com>",
    "Zaid Masud <Zaid.Masud@ADP.com>"
  ],
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lifion/lifion-insights.git"
  },
  "bugs": {
    "url": "https://github.com/lifion/lifion-insights/issues"
  },
  "homepage": "https://github.com/lifion/lifion-insights#readme",
  "main": "src/pages/index.jsx",
  "sourceType": "module",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build": "gatsby build",
    "build-docs": "jsdoc2md -t ./templates/README.hbs ./src/**/*.jsx > ./README.md && git add ./README.md",
    "build-docs-watch": "npm-watch build-docs",
    "develop": "gatsby develop",
    "eslint": "eslint . --ext .js,.json --ignore-pattern='!.*.*'",
    "format": "prettier --write '**/*.{md,js,json}' '!{coverage,.cache,public}/**/*.{js,json}'",
    "prepare": "check-engines",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest -c ./.jest.json",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "gatsby": "^2.32.11",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "auto-changelog": "^2.2.1",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^0.5.16",
    "check-engines": "^1.5.0",
    "codecov": "^3.8.1",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-lifion": "^1.4.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jsdoc-to-markdown": "^6.0.1",
    "lint-staged": "^10.5.4",
    "npm-watch": "^0.7.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build-docs && lint-staged",
      "pre-push": "npm run eslint && npm test"
    }
  },
  "watch": {
    "build-docs": {
      "patterns": [
        "src"
      ]
    }
  },
  "@lifion/core-commons": {
    "template": "public",
    "updated": "2019-11-11T21:21:36.072Z",
    "version": "2.3.4"
  }
}
